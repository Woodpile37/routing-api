+export class V2Quoter extends BaseQuoter<V2CandidatePools, V2Route> {
+  protected v2SubgraphProvider: IV2SubgraphProvider
+  protected v2PoolProvider: IV2PoolProvider
+  protected v2QuoteProvider: IV2QuoteProvider
